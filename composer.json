{
    "name": "antevenio/oauth2-mdirector",
    "description": "Oauth client library specific to access mdirector api services",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "imota",
            "email": "imota@antevenio.com"
        }
    ],
    "autoload": {
        "psr-4": {
